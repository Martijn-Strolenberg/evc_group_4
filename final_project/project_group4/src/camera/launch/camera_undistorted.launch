<launch>

  <!-- Launch the camera publisher node -->
  <node pkg="jetson_camera" type="camera_publisher_node.py" name="camera_publisher_node" output="screen">
    <rosparam command="load" file="$(find jetson_camera)/config/parameters.yaml" />
  </node>

  <!-- Launch the camera processor node -->
  <node pkg="jetson_camera" type="camera_processor_node.py" name="camera_processor_node" output="screen"/>

</launch>